  "center_window": true,
  "resizable": false,
  "vsync": true,
  "mouse_sens": "0.15 * 2",
  "font": "liberationmonob",
  "font_size": 15,
  "char_size": [9, 16],
  "charmap_high": "cp437",
  "speaker": false,
  "use_serial": false,
  "hook_time": true,
  "disable_text_mode": false,
  "name": "Example"
}
